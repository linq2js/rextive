{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@ging/rextive",
  "version": "2.36.0",
  "license": "MIT",
  "description": "A reactive state management library with explicit dependencies - works with React, Deno, and vanilla JS",
  "runtimes": [
    "deno",
    "node",
    "browser",
    "bun"
  ],
  "imports": {
    "lodash/isEqual": "npm:lodash@4/isEqual",
    "lodash/debounce": "npm:lodash@4/debounce",
    "react": "npm:react@18",
    "immer": "npm:immer@10",
    "@/index": "./src/index.ts",
    "@/utils/task": "./src/utils/task.ts",
    "@/utils/isPromiseLike": "./src/utils/isPromiseLike.ts",
    "@/utils/errorTracking": "./src/utils/errorTracking.ts",
    "@/utils/createSignalAccessProxy": "./src/utils/createSignalAccessProxy.ts",
    "@/utils/nameGenerator": "./src/utils/nameGenerator.ts",
    "@/devtools/types": "./src/devtools/types.ts",
    "@/devtools/index": "./src/devtools/index.ts",
    "@/devtools/utils/buildDependencyGraph": "./src/devtools/utils/buildDependencyGraph.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./react": "./src/react/index.ts",
    "./immer": "./src/immer/index.ts",
    "./op": "./src/op.ts",
    "./cache": "./src/cache/index.ts",
    "./plugins": "./src/plugins/index.ts",
    "./devtools": "./src/devtools/index.ts",
    "./devtools/panel": "./src/devtools/panel/index.ts",
    "./operators": "./src/operators/index.ts"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "README.md",
    "LICENSE"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "src/typeCheck.tsx",
    "src/global.d.ts"
  ],
  "publish": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "README.md",
      "LICENSE"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/*.test.tsx",
      "src/**/*.spec.ts",
      "src/**/*.spec.tsx",
      "src/typeCheck.tsx",
      "src/global.d.ts"
    ]
  }
}
